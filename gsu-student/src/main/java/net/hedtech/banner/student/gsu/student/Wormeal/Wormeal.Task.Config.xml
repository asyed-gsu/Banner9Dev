<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:task"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:task http://www.morphis-tech.com/schema/foundations/task.xsd">
	
	<!-- task parts -->
	<b:import resource="classpath:/net/hedtech/banner/student/gsu/student/Wormeal/parts/goqolib/Goqolib.TaskPart.Import.xml"/>
	<!-- library resources -->
	<b:import resource="classpath:/net/hedtech/general/common/libraries/Goqrpls/GOQRPLS.Library.Config.xml"/>
	<!-- model definition -->
	<b:import resource="classpath:/net/hedtech/banner/student/gsu/student/Wormeal/model/WORMEAL.Model.Config.xml"/>
	
	<task id="WORMEAL.Task.Config" name="WORMEAL">
		<baseTaskConfiguration config="BASE.Task.Config"/>		
		<taskParts>
			<taskPartConfiguration name="GOQOLIB" config="GOQOLIB.TaskPart.Config"/>
		</taskParts>
		<taskClass>net.hedtech.banner.student.gsu.student.Wormeal.WormealTask</taskClass>		
		<!-- struct -->
		<taskResources>/net/hedtech/banner/student/gsu/student/Wormeal/WormealStruct.xml</taskResources>
		
		<!-- support code -->
		<supportCodeConfiguration>
			<services>net.hedtech.banner.student.gsu.student.Wormeal.services.WormealServices</services>
			<attachedLibraries>
				<libraryConfiguration config="GOQRPLS.Lib.Config"/>
			</attachedLibraries>
		</supportCodeConfiguration>
		
		<!-- model -->
		<formModelConfiguration config="WORMEAL.Model.Config"/>
		
		<formController>net.hedtech.banner.student.gsu.student.Wormeal.controller.WormealFormController</formController>
		<blockControllers>
			<block name="BUTTON_CONTROL_BLOCK">net.hedtech.banner.student.gsu.student.Wormeal.controller.ButtonControlBlockController</block>
			<block name="KEY_BLOCK">net.hedtech.banner.student.gsu.student.Wormeal.controller.KeyBlockController</block>
			<block name="FORM_HEADER">net.hedtech.banner.student.gsu.student.Wormeal.controller.FormHeaderController</block>
			<block name="WBRMEAL">net.hedtech.banner.student.gsu.student.Wormeal.controller.WbrmealController</block>
		</blockControllers>
	</task>
</b:beans>