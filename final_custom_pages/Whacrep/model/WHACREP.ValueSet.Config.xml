<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:valueset"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:valueset http://www.morphis-tech.com/schema/foundations/valueset.xsd">
	
	<valueSet id="STVSUBJ.ValueSet.Config" name="STVSUBJ">
		<columns>
			<column name="STVSUBJ_CODE" type="String" size="4"/>
			<column name="STVSUBJ_DESC" type="String" size="30"/>
		</columns>
		<query><![CDATA[SELECT STVSUBJ_CODE, STVSUBJ_DESC
  FROM STVSUBJ
  ORDER BY STVSUBJ_CODE ASC]]>
		</query>
		<parameters>
		</parameters>
	</valueSet>
</b:beans>