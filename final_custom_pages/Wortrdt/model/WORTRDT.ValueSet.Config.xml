<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:valueset"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:valueset http://www.morphis-tech.com/schema/foundations/valueset.xsd">
	
	<valueSet id="WTVTRME_RG.ValueSet.Config" name="WTVTRME_RG">
		<columns>
			<column name="WTVTRME_EVENT_CODE" type="String" size="6"/>
			<column name="WTVTRME_DESCRIPTION" type="String" size="30"/>
		</columns>
		<query><![CDATA[SELECT WTVTRME_EVENT_CODE, WTVTRME_DESCRIPTION FROM WTVTRME
		
		]]>
		</query>
		<parameters>
		</parameters>
	</valueSet>
	<valueSet id="STVPTRM_RG.ValueSet.Config" name="STVPTRM_RG">
		<columns>
			<column name="STVPTRM_CODE" type="String" size="6"/>
			<column name="STVPTRM_DESC" type="String" size="30"/>
		</columns>
		<query><![CDATA[SELECT STVPTRM_CODE, STVPTRM_DESC FROM STVPTRM ORDER BY STVPTRM_DESC ASC
		
		]]>
		</query>
		<parameters>
		</parameters>
	</valueSet>
</b:beans>